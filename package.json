  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sprat/nany.git"
  },
  "homepage": "http://sprat.github.io/nany/",
  "bugs": {
    "url": "https://github.com/sprat/nany/issues"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "core-js": "^2.5.3",
    "htmldec": "^0.0.1",
    "maquette": "^2.5.4",
    "mini-signals": "^1.2.0",
    "print-html": "^0.0.2",
    "pubnub": "^4.20.2",
    "qs": "^6.5.1",
    "xhr": "^2.4.1",
    "zip-object": "^0.1.0"
  },
  "devDependencies": {
    "css-loader": "^0.28.10",
    "electron": "^1.8.4",
    "eslint": "^4.18.2",
    "extract-text-webpack-plugin": "^2.1.2",
    "faucet": "^0.0.1",
    "gh-pages": "^1.1.0",
    "html-loader": "^0.4.4",
    "json-loader": "^0.5.4",
    "live-server": "^1.2.0",
    "nodemon": "^1.17.2",
    "npm-run-all": "^4.1.2",
    "rimraf": "^2.6.2",
    "tape": "^4.9.0",
    "tape-catch": "^1.0.6",
    "tape-run-electron": "^1.0.18",
    "webpack": "^3.11.0"
  },
  "scripts": {
    "start": "live-server --no-browser www",
    "lint": "eslint . --cache",
    "prebuild": "npm run lint -s",
    "build": "webpack --bail",
    "pretest": "npm run build -s",
    "test": "tape-run-electron < www/dist/nany.unit_tests.js | faucet",
    "watch": "nodemon -e js,json,css --ignore www/dist --exec \"npm test -s\"",
    "dev": "npm-run-all --parallel start watch",
    "predeploy": "git diff-index --quiet HEAD && git rev-parse --quiet HEAD >www/COMMIT_ID",
    "deploy": "gh-pages -d www -m Deploy"
  }
}
