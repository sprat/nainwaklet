{
  "name": "nany",
  "version": "1.0.0",
  "description": "Nainwak Hub Bookmarklet",
  "author": "Sylvain Prat <sylvain.prat@gmail.com>",
  "contributors": [],
  "keywords": [
    "Nainwak"
  ],
  "license": "MIT",
  "private": true,
  "main": "js/nany/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sprat/nany.git"
  },
  "homepage": "http://sprat.github.io/nany/",
  "bugs": {
    "url": "https://github.com/sprat/nany/issues"
  },
  "dependencies": {
    "array-find": "^1.0.0",
    "get-dataset": "^0.0.4",
    "htmldec": "^0.0.1",
    "mini-signals": "^1.1.1",
    "print-html": "^0.0.2",
    "pubnub": "^4.0.9",
    "qs": "^6.2.1",
    "xhr": "^2.2.2",
    "xtend": "^4.0.1",
    "zip-object": "^0.1.0"
  },
  "devDependencies": {
    "brfs": "^1.4.3",
    "browserify": "^13.1.0",
    "clean-css": "^3.4.19",
    "eslint": "^3.5.0",
    "exorcist": "^0.4.0",
    "faucet": "^0.0.1",
    "live-server": "^1.1.0",
    "nodemon": "^1.10.2",
    "npm-run-all": "^3.1.0",
    "rimraf": "^2.5.4",
    "smokestack": "^3.4.1",
    "tap-closer": "^1.0.0",
    "tape": "^4.6.0",
    "tape-catch": "^1.0.6",
    "uglify-js": "^2.7.3"
  },
  "scripts": {
    "start": "live-server --no-browser",
    "lint": "eslint . --cache",
    "pretest": "browserify js/nany/tests -t brfs -d > dist/nany.tests.js",
    "test": "tap-closer < dist/nany.tests.js | smokestack | faucet",
    "prebuild:js": "npm run lint -s && npm run test -s",
    "build:js": "browserify js/nany/index.js -dv -s Nany | exorcist dist/nany.js.map > dist/nany.js",
    "postbuild:js": "uglifyjs dist/nany.js -mc warnings=false -o dist/nany.min.js --in-source-map dist/nany.js.map --source-map dist/nany.min.js.map --source-map-url nany.min.js.map && rimraf dist/nany.js*",
    "build:css": "cleancss --source-map --source-map-inline-sources --output dist/nany.min.css css/nany/index.css",
    "build": "npm run build:js -s && npm run build:css -s",
    "watch:js": "nodemon -e js --ignore dist --exec npm run build:js -s",
    "watch:css": "nodemon -e css --ignore dist --exec npm run build:css -s",
    "watch": "npm-run-all --parallel start watch:*"
  }
}
