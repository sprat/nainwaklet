  "contributors": [],
  "keywords": [
    "Nainwak"
  ],
  "license": "MIT",
  "private": true,
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sprat/nany.git"
  },
  "homepage": "http://sprat.github.io/nany/",
  "bugs": {
    "url": "https://github.com/sprat/nany/issues"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "core-js": "^2.4.1",
    "htmldec": "^0.0.1",
    "maquette": "^2.3.5",
    "mini-signals": "^1.1.1",
    "print-html": "^0.0.2",
    "pubnub": "^4.0.9",
    "qs": "^6.2.1",
    "xhr": "^2.2.2",
    "zip-object": "^0.1.0"
  },
  "devDependencies": {
    "css-loader": "^0.25.0",
    "eslint": "^3.5.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "faucet": "^0.0.1",
    "html-loader": "^0.4.4",
    "json-loader": "^0.5.4",
    "live-server": "^1.1.0",
    "nodemon": "^1.10.2",
    "npm-run-all": "^3.1.0",
    "rimraf": "^2.5.4",
    "tap-closer": "^1.0.0",
    "tape": "^4.6.0",
    "tape-catch": "^1.0.6",
    "tape-run": "^2.1.4",
    "webpack": "^1.13.2"
  },
  "scripts": {
    "start": "live-server --no-browser --ignorePattern=src/.*",
    "lint": "eslint . --cache",
    "prebuild": "npm run lint -s",
    "build": "webpack --bail",
    "test": "tap-closer < dist/nany.tests.js | tape-run | faucet",
    "unittest": "tap-closer < dist/nany.unittests.js | tape-run | faucet",
    "watch": "nodemon -e js,json,css --ignore dist --exec \"npm run build -s && npm run unittest -s\"",
    "dev": "npm-run-all --parallel start watch"
  }
}
